<header id="top-header">
    <span class="siteName">
        <a href="/" title="Return to home page">CSE Motors</a>
    </span>
    <% if (locals.loggedIn) { %>
    <div class="account-links-container">
        <div class="account-link"><a href="/account/">Welcome <%- locals.accountName %></a></div>
        <div class="account-link"><a href="/account/logout">Logout</a></div>
    </div>
    <% } else { %>
        <div class="account-link"><a href="/account/login">My Account</a></div>
    <% } %>
</header>